{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 */
#}
{#{{ dump(content) }}#}

{% set file_url = file_url(content.field_media_image['#object'].field_media_image.entity.uri.value|image_style('inline_media_small')) %}
{% set file_name = content.field_media_image['#object'].field_media_image.entity.filename.value %}

<img src="{{ file_url }}" alt="{{ file_name }}">
