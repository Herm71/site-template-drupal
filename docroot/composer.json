{
    "name": "drupal/drupal",
    "description": "Drupal is an open source content management platform powering millions of websites and applications.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "composer/installers": "^1.0.24",
        "cweagans/composer-patches": "1.x-dev",
        "wikimedia/composer-merge-plugin": "^1.4",
        "drupal/devel": "^2.0",
        "drupal/ctools": "^3.0",
        "drupal/module_filter": "^3.1",
        "drupal/group": "^1.0@RC",
        "drupal/groupmenu": "^1.0@alpha",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/tb_megamenu": "1.x-dev",
        "drupal/entity_browser": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity": "^1.0-beta4",
        "drupal/entity_block": "^1.0",
        "drupal/focal_point": "^1.0",
        "drupal/linkit": "^5.0",
        "drupal/ckeditor_templates": "^1.0",
        "drupal/google_analytics": "^2.3",
        "drupal/video_embed_field": "^2.0",
        "drupal/linked_field": "^1.1",
        "drupal/metatag": "^1.7",
        "drupal/masquerade": "^2.0",
        "drupal/webform": "^5.1",
        "drupal/geolocation": "^1.11",
        "drupal/field_group": "3.x-dev",
        "drupal/calendar": "1.x-dev",
        "drupal/block_class": "^1.0",
        "drupal/twig_link_attributes": "1.x-dev",
        "drupal/twig_vardumper": "^1.1",
        "drupal/libraries": "^3.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/name": "1.x-dev",
        "drupal/feeds": "3.x-dev",
        "drupal/feeds_tamper": "2.x-dev",
        "drupal/auto_entitylabel": "^2.1",
        "drupal/dropzonejs": "^2.0",
        "drupal/twig_field_value": "^1.1",
        "drupal/address": "^1.4",
        "drupal/menu_block": "^1.5",
        "drupal/ckeditor_media_embed": "^1.3",
        "drupal/file_browser": "^1.1",
        "drupal/viewsreference": "^2.0",
        "drupal/views_bulk_operations": "^2.4",
        "drupal/views_bulk_edit": "^2.2",
        "drupal/smart_ip": "^3.6",
        "drupal/wysiwyg_template": "^2.1",
        "drupal/asset_injector": "^2.4",
        "drupal/redirect": "^1.3",
        "drupal/rules": "^3.0",
        "drupal/file_mdm": "^1.1",
        "drupal/restui": "^1.16",
        "drupal/search_api": "^1.11",
        "drupal/xmlsitemap": "^1.0",
        "drupal/facets": "^1.2",
        "drupal/search_api_autocomplete": "^1.1",
        "drupal/search_api_solr": "^2.5",
        "drush/drush": "^9.5",
        "drupal/date_popup": "^1.0",
        "topfloor/composer-cleanup-vcs-dirs": "^1.0",
        "drupal/smtp": "^1.0",
        "drupal/captcha": "^1.0@beta",
        "drupal/captcha_pack": "^1.0",
        "drupal/recaptcha": "^2.4"
    },
    "replace": {
        "drupal/core": "^8.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "autoloader-suffix": "Drupal8"
    },
    "extra": {
        "_readme": [
            "By default Drupal loads the autoloader from ./vendor/autoload.php.",
            "To change the autoloader you can edit ./autoload.php.",
            "This file specifies the packages.drupal.org repository.",
            "You can read more about this composer repository at:",
            "https://www.drupal.org/node/2718229"
        ],
        "merge-plugin": {
            "include": [
                "core/composer.json",
                "modules/custom/*/composer.json",
                "themes/custom/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "modules/custom/{$name}": ["type:drupal-custom-module"],
            "themes/custom/{$name}": ["type:drupal-custom-theme"],
            "libraries/{$name}": ["type:drupal-library"]
        },
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/field_group" : {
              "Tokens in field group's css classes": "https://www.drupal.org/files/issues/2019-01-15/field_group-tokens_in_classes-2858336-16.patch"
            },
            "drupal/feeds": {
              "Add a new FeedsTarget plugin to make name fields mappable in feeds importers.": "https://www.drupal.org/files/issues/NameTarget-2785491-2.patch"
            },
            "drupal/group": {
              "Configurable entities as group content": "https://www.drupal.org/files/issues/group-configurable-entities-as-group-content-2797793-48.patch",
              "Notice: Undefined index: section": "./patches/group-undefined-index.patch"
            },
            "drupal/groupmenu": {
              "UndefinedLinkTemplateException": "https://www.drupal.org/files/issues/2880714.patch",
              "Urls without route in admin menu cause fatal error": "https://www.drupal.org/files/issues/non-routed-urls-in-admin-menu-2927842.patch",
              "Menus disappeared from menu administration menu after module installation": "https://www.drupal.org/files/issues/2018-07-06/menus-disappeared-after-installation-2896706-11.patch",
              "Undefined index: node_form_group_menu in groupmenu_form_alter": "https://www.drupal.org/files/issues/2019-01-24/UndefinedIndexError-3028195-1.patch"
            },
            "drupal/entity_embed": {
              "[PP-1] Image entities/fields embedded using Entity Embed cannot be linked in CKEditor": "https://www.drupal.org/files/issues/2018-09-17/entity-embed-link-2511404-68.patch"
            },
            "drupal/calendar": {
              "Notice: Undefined property: CalendarEvent $date_id": "https://www.drupal.org/files/issues/calendar-undefined_property_date_id-2876414-3.patch"
            }
            
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-autoload-dump": "Drupal\\Core\\Composer\\Composer::preAutoloadDump",
        "post-autoload-dump": "Drupal\\Core\\Composer\\Composer::ensureHtaccess",
        "post-package-install": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "drupal-phpunit-upgrade-check": "Drupal\\Core\\Composer\\Composer::upgradePHPUnit",
        "drupal-phpunit-upgrade": "@composer update phpunit/phpunit phpspec/prophecy symfony/yaml --with-dependencies --no-progress",
        "phpcs": "phpcs --standard=core/phpcs.xml.dist --runtime-set installed_paths $($COMPOSER_BINARY config vendor-dir)/drupal/coder/coder_sniffer --",
        "phpcbf": "phpcbf --standard=core/phpcs.xml.dist --runtime-set installed_paths $($COMPOSER_BINARY config vendor-dir)/drupal/coder/coder_sniffer --"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.11.1",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.11.1.zip",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "4.2.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ]
}
